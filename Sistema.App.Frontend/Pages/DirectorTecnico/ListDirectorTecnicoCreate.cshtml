@page
@model Sistema.App.Frontend.Page.ListDirectorTecnicoCreateModel
@{
}
<h2>Crear director técnico :</h2>
<form method="post">
    <div class="form-group">
        <label asp-for="directorTecnico.Nombre"></label>
        <input asp-for="directorTecnico.Nombre" class= "form-control">
        <span class="text-danger" asp-validation-for="directorTecnico.Nombre"></span>
    </div>
    <div class="form-group">
        <label asp-for="directorTecnico.Apellido"></label>
        <input asp-for="directorTecnico.Apellido" class= "form-control">
        <span class="text-danger" asp-validation-for="directorTecnico.Apellido"></span>
    </div>
    <div class="form-group">
        <label asp-for="directorTecnico.Documento"></label>
        <input asp-for="directorTecnico.Documento" class= "form-control">
        <span class="text-danger" asp-validation-for="directorTecnico.Documento"></span>
    </div>
    <div class="form-group">
        <label asp-for="directorTecnico.Telefono"></label>
        <input asp-for="directorTecnico.Telefono" class= "form-control">
        <span class="text-danger" asp-validation-for="directorTecnico.Telefono"></span>
    </div>
    <button type="submit" class="btn btn-primary">Crear Director Técnico</button>
</form>
<a asp-page="./ListDirectorTecnico" class="btn btn-primary">Volver</a>